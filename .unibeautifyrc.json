{
  "CSS": {
    "beautifiers": [
      "Prettier",
      "Pretty Diff",
      "JS-Beautify"
    ],
    "indent_size": 4,
    "indent_style": "space",
    "quotes": "double",
    "end_with_semicolon": true,
    "max_preserve_newlines": 3,
    "pragma_insert": false,
    "no_leading_zero": false,
    "selector_separator_newline": false
  },
  "HTML": {
    "beautifiers": [
      "Pretty Diff",
      "JS-Beautify"
    ],
    "indent_size": 4,
    "indent_style": "space",
    "quotes": "double",
    "max_preserve_newlines": 3,
    "indent_inner_html": true,
    "indent_scripts": "normal",
    "unformatted": [],
  },
  "JSON": {
    "beautifiers": [
      "Prettier",
      "Pretty Diff",
      "JS-Beautify"
    ],
    "indent_size": 4,
    "indent_style": "space",
    "quotes": "double",
    "pragma_insert": false,
    "arrow_parens": "always",
    "comma_first": false,
    "no_leading_zero": false,
    "space_before_conditional": true,
    "space_in_empty_paren": true
  },
  "JavaScript": {
    "beautifiers": [
      "Prettier",
      "Pretty Diff",
      "JS-Beautify",
      "ESLint"
    ],
    "indent_size": 4,
    "indent_style": "space",
    "quotes": "single",
    "end_with_comma": true,
    "end_with_semicolon": false,
    "max_preserve_newlines": 3,
    "pragma_insert": false,
    "brace_style": "collapse",
    "arrow_parens": "always",
    "comma_first": false,
    "space_before_conditional": true,
    "space_in_empty_paren": true,
    "space_in_paren": false,
    "indent_chained_methods": true,
  },
  "Markdown": {
    "beautifiers": [
      "Prettier"
    ],
    "indent_size": 4,
    "indent_style": "space",
    "pragma_insert": false,
    "wrap_prose": "preserve"
  }
}
